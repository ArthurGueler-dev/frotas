# Configurações da API Ituran
ITURAN_API_URL=https://iweb.ituran.com.br
ITURAN_USERNAME=seu_usuario_ituran
ITURAN_PASSWORD=sua_senha_ituran
ITURAN_TIMEOUT=120000

# Configurações do Servidor
NODE_ENV=development
SERVER_PORT=5000

# Configurações do Banco de Dados (MySQL)
DB_HOST=seu_host_mysql
DB_PORT=3306
DB_USER=seu_usuario_mysql
DB_PASSWORD=sua_senha_mysql
DB_NAME=seu_banco_dados
DB_CHARSET=utf8mb4

# Configurações do Sistema
CORS_ENABLED=true
MILEAGE_CACHE_MINUTES=5
